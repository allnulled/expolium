const RestModelController = require(process.env.PROJECT_ROOT + "/core/controller/RestModelController.js");
const <%=Model%> = require(process.env.PROJECT_ROOT + "/core/model/rest/<%=databaseConnection%>/<%=Model%>.js");
const StringUtils = require(process.env.PROJECT_ROOT + "/core/helper/StringUtils.js");

class Base<%=Model%>Controller extends RestModelController {

	static get model() {
		return <%=Model%>;
	}

	static get name() {
		return <%-JSON.stringify(Model)%>;
	}

	static get path() {
		return <%-JSON.stringify(StringUtils.hyphenize(Model))%>;
	}
	
}

module.exports = Base<%=Model%>Controller;