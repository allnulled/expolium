<div>
	<div class="" data-rest-search-parameters-form>
		<div class="margin-5 margin-top-0">
			<ul class="row">
				<li class="col-12">
					<div>
						<button class="btn btn-sm btn-success" type="button" onclick="TemplateManager.renderTextToTag({
							text: jQuery(this).next().next().text(), 
							parameters: {}, 
							options: {delimiter:'@'}, 
							tag: jQuery(this).closest('div').next('ul')
						})">
							<i class="fas fa-plus"></i>
						</button>
						<label>Filter:</label>
						<script type="text/template">
							<li>
								<div style="display:table">
									<div style="display:table-row">
										<div style="display:table-cell;vertical-align:middle;padding:2px;padding-left:0;">
											<span class="btn btn-sm btn-outline sortable-handler" style="width:100%;height:100%; cursor: grab;">
												<i class="fas fa-bars"></i>
											</span>
										</div>
										<div style="display:table-cell;vertical-align:middle;padding:2px;width:40%">
											<select class="form-control form-control-sm" name="where[][0]">
												<% Object.keys(cols).forEach(col => { %>
												<option value="<%=col%>"><%=col%></option>
												<% }) %>
											</select>
										</div>
										<div style="display:table-cell;vertical-align:middle;padding:2px;width:20%">
											<select class="form-control form-control-sm" name="where[][1]">
												<option value="=">=</option>
												<option value="<">&lt;</option>
												<option value=">">&gt;</option>
												<option value="<=">&lt;=</option>
												<option value=">=">&gt;=</option>
												<option value="like">like</option>
												<option value="not like">not like</option>
												<option value="ilike">like (ignore case)</option>
												<option value="not ilike">not like (ignore case)</option>
												<option value="in">in</option>
												<option value="not in">not in</option>
												<option value="$">$</option>
											</select>
										</div>
										<div style="display:table-cell;vertical-align:middle;padding:2px;width:40%">
											<input class="form-control form-control-sm" type="text" name="where[][2]" />
										</div>
										<div style="display:table-cell;vertical-align:middle;padding:2px;">
											<span>
												<button class="btn btn-sm btn-danger item-closer" style="vertical-align:middle;" onclick="jQuery(this).closest('li').remove()">
													<i class="fas fa-trash-alt" style="vertical-align:middle;"></i>
												</button>
											</span>
										</div>
									</div>
								</div>
							</li>
						</script>
					</div>
					<ul class="sortable-list">
					</ul>
				</li>

				<li class="col-12<%-(typeof isViewMany === "boolean") && isViewMany ? ' col-md-6' : '' %>">
					<hr class="margin-5" />
					<div>
						<button class="btn btn-sm btn-success" type="button" onclick="TemplateManager.renderTextToTag({
							text: jQuery(this).next().next().text(), 
							parameters: {}, 
							options: {delimiter:'@'}, 
							tag: jQuery(this).closest('div').next('ul')
						})">
							<i class="fas fa-plus"></i>
						</button>
						<label>Order:</label>
						<script type="text/template">
							<li>
								<div style="display:table">
									<div style="display:table-row">
										<div style="display:table-cell;vertical-align:middle;padding:2px;padding-left:0;">
											<span class="btn btn-sm btn-outline sortable-handler" style="width:100%;height:100%; cursor: grab;">
												<i class="fas fa-bars"></i>
											</span>
										</div>
										<div style="display:table-cell;vertical-align:middle;padding:2px;width:100%">
											<select class="form-control form-control-sm" name="sort[][0]">
												<% Object.keys(cols).forEach(col => { %>
												<option value="<%=col%>"><%=col%></option>
												<% }) %>
											</select>
										</div>
										<div style="display:table-cell;vertical-align:middle;padding:2px;">
											<label class="form-label" style="word-break: keep-all;white-space: nowrap; vertical-align:middle;">
												<input type="checkbox" class="form-control-input" name="sort[][1]" style="vertical-align:middle;" value-off="asc" value="desc" />
												<span style="vertical-align:middle;font-size:10px">Descending</span>
											</label>
										</div>
										<div style="display:table-cell;vertical-align:middle;padding:2px;">
											<span>
												<button class="btn btn-sm btn-danger item-closer" style="vertical-align:middle;" onclick="jQuery(this).closest('li').remove()">
													<i class="fas fa-trash-alt" style="vertical-align:middle;"></i>
												</button>
											</span>
										</div>
									</div>
								</div>
							</li>
						</script>
					</div>
					<ul class="sortable-list">
					</ul>
				</li>

				<li class="col-12<%-(typeof isViewMany === "boolean") && isViewMany ? ' col-md-6' : '' %>">
					<hr class="margin-5" />
					<div>
						<button class="btn btn-sm btn-success" type="button" onclick="TemplateManager.renderTextToTag({
							text: jQuery(this).next().next().text(), 
							parameters: {}, 
							options: {delimiter:'@'}, 
							tag: jQuery(this).closest('div').next('ul')
						})">
							<i class="fas fa-plus"></i>
						</button>
						<label>Group:</label>
						<script type="text/template">
							<li>
								<div style="display:table">
									<div style="display:table-row">
										<div style="display:table-cell;vertical-align:middle;padding:2px;padding-left:0;">
											<span class="btn btn-sm btn-outline sortable-handler" style="width:100%;height:100%; cursor: grab;">
												<i class="fas fa-bars"></i>
											</span>
										</div>
										<div style="display:table-cell;vertical-align:middle;padding:2px;width:100%">
											<select class="form-control form-control-sm" name="group[]">
												<% Object.keys(cols).forEach(col => { %>
												<option value="<%=col%>"><%=col%></option>
												<% }) %>
											</select>
										</div>
										<div style="display:table-cell;vertical-align:middle;padding:2px;">
											<span>
												<button class="btn btn-sm btn-danger item-closer" style="vertical-align:middle;" onclick="jQuery(this).closest('li').remove()">
													<i class="fas fa-trash-alt" style="vertical-align:middle;"></i>
												</button>
											</span>
										</div>
									</div>
								</div>
							</li>
						</script>
					</div>
					<ul class="sortable-list">
					</ul>
				</li>

				<li class="col-12">
					<hr class="margin-5 margin-bottom-0" />
					<div class="container">
						<div class="row margin-bottom-0">
							<div class="col-6">
								<label style="width:100%">
									<span>Limit:</span>
									<select class="form-control form-control-sm" name="limit">
										<option value="10">10</option>
										<option value="20" selected>20</option>
										<option value="50">50</option>
										<option value="100">100</option>
										<option value="200">200</option>
										<option value="0">all</option>
									</select>
								</label>
							</div>
							<div class="col-6">
								<label style="width:100%">
									<span>Page:</span>
									<input class="form-control form-control-sm" type="text" name="page" value="1" />
								</label>
							</div>
						</div>
					</div>
				</li>

			</ul>
		</div>
	</div>
</div>